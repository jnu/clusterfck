env:
  browser: true
  node: true

rules:
# indent: 4
  no-unused-vars: [2, { "vars": "all", "args": "none" }]
  strict: 0
  global-strict: 0
  one-var: [2, "never"]
  quotes: 0
  no-new-func: 2
  no-eval: 2
  no-underscore-dangle: 0
  no-new: 0
  no-console: 2
  camelcase: 0
  no-multi-spaces: 0
  eqeqeq: 2
  curly: 2
  no-shadow: 1
  space-infix-ops: 0
  new-cap: 0
  comma-spacing: 0
  dot-notation: 0
  key-spacing: 0
  max-len: [2, 120, 4]
  handle-callback-err: 1

globals:
  DEBUG: true
  module: true
